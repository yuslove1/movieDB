  {
    "version": 2,
    "rewrites":  [
        {"source": "/(.*)", "destination": "/"}
      ],
    "builds": [{ "src": "build/**", "use": "@vercel/static-build" }],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
    
  }
